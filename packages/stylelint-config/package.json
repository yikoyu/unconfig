{
  "name": "@yikoyu/stylelint-config",
  "type": "module",
  "version": "0.1.2",
  "private": false,
  "packageManager": "pnpm@10.11.1",
  "description": "yikoyu's stylelint config",
  "author": "yikoyu<2282373181@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yikoyu/unconfig",
    "directory": "/packages/stylelint-config"
  },
  "keywords": [
    "stylelint",
    "config",
    "stylelintconfig",
    "stylelint-config",
    "lint",
    "css",
    "scss",
    "less"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --config ../../tsdown.config.ts --watch",
    "build": "tsdown --minify --clean --dts --config ../../tsdown.config.ts"
  },
  "peerDependencies": {
    "stylelint": "^16.0.0"
  },
  "dependencies": {
    "postcss": "^8.4.49",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0"
  },
  "devDependencies": {
    "@types/postcss-less": "^4.0.6",
    "stylelint": "^16.10.0"
  }
}
