{
  "name": "@yikoyu/stylelint-config",
  "type": "module",
  "version": "0.1.6",
  "private": false,
  "packageManager": "pnpm@10.25.0",
  "description": "yikoyu's stylelint config",
  "author": "yikoyu<2282373181@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yikoyu/unconfig.git",
    "directory": "packages/stylelint-config"
  },
  "keywords": [
    "stylelint",
    "config",
    "stylelintconfig",
    "stylelint-config",
    "lint",
    "css",
    "scss",
    "less"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./*": "./*"
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown --minify"
  },
  "peerDependencies": {
    "stylelint": "^16.23.1"
  },
  "dependencies": {
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0"
  },
  "devDependencies": {
    "@types/postcss-less": "^4.0.7",
    "stylelint": "^16.26.1"
  }
}
