{
  "name": "@yikoyu/commitlint-config",
  "type": "module",
  "version": "0.2.4",
  "private": false,
  "packageManager": "pnpm@10.25.0",
  "description": "yikoyu's commitlint config",
  "author": "yikoyu<2282373181@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yikoyu/unconfig.git",
    "directory": "packages/commitlint-config"
  },
  "keywords": [
    "config",
    "commitlint",
    "conventionalcommits"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./*": "./*"
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown --minify"
  },
  "peerDependencies": {
    "cz-git": ">=1.3.11",
    "czg": ">=1.3.11"
  },
  "peerDependenciesMeta": {
    "cz-git": {
      "optional": true
    },
    "czg": {
      "optional": true
    }
  },
  "dependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0"
  },
  "devDependencies": {
    "cz-git": "^1.12.0",
    "czg": "^1.12.0"
  }
}
