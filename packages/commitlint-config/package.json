{
  "name": "@yikoyu/commitlint-config",
  "type": "module",
  "version": "0.2.0",
  "private": false,
  "packageManager": "pnpm@8.15.8",
  "description": "yikoyu's commitlint config",
  "author": "yikoyu<2282373181@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yikoyu/unconfig",
    "directory": "/packages/commitlint-config"
  },
  "keywords": [
    "config",
    "commitlint",
    "conventionalcommits"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsup --config ../../tsup.config.ts --watch",
    "build": "tsup --minify --clean --dts --config ../../tsup.config.ts"
  },
  "peerDependencies": {
    "cz-git": ">=1.3.11",
    "czg": ">=1.3.11"
  },
  "peerDependenciesMeta": {
    "cz-git": {
      "optional": true
    },
    "czg": {
      "optional": true
    }
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "cz-git": "^1.9.4",
    "czg": "^1.9.4"
  }
}
